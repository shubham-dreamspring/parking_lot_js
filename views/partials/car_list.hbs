<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Registration No.</th>
      <th scope="col">Slot</th>
    </tr>
  </thead>
  <tbody>
    {{#each listdata}}
      <tr>
        <th>{{@index}}</th>
        <td>{{registration_no}}</td>
        <td>{{slot}}</td>
      </tr>
    {{/each}}
  </tbody>

</table>
{{log data level="debug"}}