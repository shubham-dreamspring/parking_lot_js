{{#if listdata.length}}
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Registration No.</th>
        <th scope="col">Slot</th>
      </tr>
    </thead>
    <tbody>
      {{#each listdata}}
        <tr>
          <th>{{@index}}</th>
          <td>{{registration_no}}</td>
          <td>{{slot}}</td>
        </tr>
      {{/each}}
    </tbody>

  </table>
{{else}}
  <h4 class="text-center lead">No car yet parked</h4>
{{/if}}